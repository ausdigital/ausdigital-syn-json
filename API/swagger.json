  "basePath" : "/ausdigital/ubl-json/1.0.0",
  "schemes" : [ "https" ],
  "paths" : {
    "/json2ubl" : {
      "post" : {
        "summary" : "converts JSON UBL to UBL 2.1 XML",
        "description" : "Pass a valid UBL JSON instance document and this API will return a fully conformant UBL 2.1 XML instance.\n",
        "operationId" : "json2ubl",
        "consumes" : [ "application/json" ],
        "produces" : [ "application/xml" ],
        "parameters" : [ {
          "in" : "body",
          "name" : "ubljson",
          "description" : "valid UBL JSON instance",
          "required" : true,
          "schema" : {
            "type" : "string"
          }
        } ],
        "responses" : {
          "200" : {
            "description" : "UBL 2.1 XML document",
            "schema" : {
              "type" : "string"
            }
          },
          "400" : {
            "description" : "bad input parameter"
          }
        }
      }
    },
    "/ubl2json" : {
      "post" : {
        "summary" : "converts UBL 2.1 XML to JSON",
        "description" : "Pass a valid UBL 2.1 XML instance document and this API will return a semantically equivalent JSON instance document.\n",
        "operationId" : "ubl2json",
        "consumes" : [ "application/xml" ],
        "produces" : [ "application/json" ],
        "parameters" : [ {
          "in" : "body",
          "name" : "ublxml",
          "description" : "valid UBL 2.1 XML instance",
          "required" : true,
          "schema" : {
            "type" : "string"
          }
        } ],
        "responses" : {
          "200" : {
            "description" : "UBL document in JSON representation",
            "schema" : {
              "type" : "string"
            }
          },
          "400" : {
            "description" : "bad input parameter"
          }
        }
      }
    }
  },
  "definitions" : { }
